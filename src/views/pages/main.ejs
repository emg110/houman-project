  </div>
  <div id="carosel" style="width:800px;height:450px;margin-left:14%">
    <div id="powerpoint" class="carousel slide" data-ride="carousel">

      <!-- Indicators -->
      <ul class="carousel-indicators">
        <li data-target="#powerpoint" data-slide-to="0" class="active"></li>
        <li data-target="#powerpoint" data-slide-to="1"></li>
        <li data-target="#powerpoint" data-slide-to="2"></li>
      </ul>

      <!-- The slideshow -->
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide1.jpg" alt="Slide #1">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide2.jpg" alt="Slide #2">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide3.jpg" alt="Slide #3">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide4.jpg" alt="Slide #4">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide5.jpg" alt="Slide #5">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide6.jpg" alt="Slide #6">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide7.jpg" alt="Slide #7">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide8.jpg" alt="Slide #8">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide9.jpg" alt="Slide #9">
        </div>
        <div class="carousel-item">
          <img style="max-width:800px;max-height:450px" src="/img/ppt/Slide10.jpg" alt="Slide #10">
        </div>
      </div>

      <!-- Left and right controls -->
      <a class="carousel-control-prev" href="#powerpoint" data-slide="prev">
        <span class="carousel-control-prev-icon"></span>
      </a>
      <a class="carousel-control-next" href="#powerpoint" data-slide="next">
        <span class="carousel-control-next-icon"></span>
      </a>

    </div>
