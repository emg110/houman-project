<% if(!(typeof etlData.edges == 'undefined')){ console.log('we got data here...')%>
  <% etlData.edges.forEach((edge) => { %>
  <% edge = edge.node %>
  <% var commentDate = new Date(parseInt(edge.created_at)*1000) %>
  <% edge.created_at = commentDate.getFullYear()+'-'+commentDate.getMonth()+'-'+commentDate.getDay() %>
  <div class="row whiteBox ">

    <div class="col-sm-6 text-center align-self-center">
      <% if(!(typeof edge == 'undefined')){%>
        <% if(edge.owner.profile_pic_url) { %>
          <img class="img-thumbnail" src="<%= !(typeof edge == 'undefined') ? edge.owner.profile_pic_url:"" %>" alt="<%= edge ? edge.owner.username :"" %>">
        <% } %>
      <%}%>
    </div>

    <div class="col-sm-6 text-center align-self-center">

      <h2><%= !(typeof edge == 'undefined') ? edge.owner.username : "username" %></h2>
      <h3><%= !(typeof edge == 'undefined') ? edge.owner.full_name :"-"%></h3>
      <h3><%= !(typeof edge == 'undefined') ? edge.created_at : "0000/00/00" %></h3>
      </br>
      <h4><%= !(typeof edge == 'undefined') ? edge.text : ":..." %></h4>
    </div>

  </div>

<% }) %>
<%}%>
